  {
    "name": "Crowdference",
    "version": "1.2.0",
    "description": "The crowdference plugin make it even easier to create collaborative custom links between any two web pages.",
    "permissions": [
      "http://localhost:2900/*",
      "http://localhost:2800/*",
      "*://*.crowdference.org/*",
      "tabs",
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Colaborative user-customized links between web pages",
      "default_icon": {
        "16": "images/ico16.png",
        "32": "images/ico32.png",
        "48": "images/ico48.png",
        "128": "images/ico128.png"
      }
    },
    "icons": {
      "16": "images/ico16.png",
      "32": "images/ico32.png",
      "48": "images/ico48.png",
      "128": "images/ico128.png"
    },
    "default_locale": "en",
    "web_accessible_resources": [
      "popup.js.map"
    ],
    "manifest_version": 2
  }
